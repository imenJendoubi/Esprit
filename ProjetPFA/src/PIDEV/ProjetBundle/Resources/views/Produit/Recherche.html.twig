{{ form(form) }}

<h1>List Produit</h1>
<form method="post" action="">
    <table border="1">
        <tr>
            <th>id</th><th>Nom Produit</th><th>Description</th><th>Prix</th><th>Image</th><th>Stock</th><th>Categorie</th>

        </tr>


        {% for i in m %}
            <tr>
                <td>{{ i.id }}</td>
                <td>{{ i.nomP }}</td>
                <td>{{ i.descP }}</td>
                <td>{{ i.prix }}</td>
                <td>{{ i.imageP }}</td>
                <td>{{ i.qteStock }}</td>
                <td>{{ i.categorie }}</td>

            </tr>
        {% endfor %}
    </table>
</form>